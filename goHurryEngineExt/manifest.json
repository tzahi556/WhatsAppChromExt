
{
  "manifest_version": 2,
  "name": "GohurryEngine",
  "version": "1.1",
  "permissions": [
		"activeTab",
		"storage",
		"webNavigation", 
		"webRequest",
    "tabs",
    "downloads"
	
	],
  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "Have a good day"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery.min.js","content.js"]
    }
  ]
}