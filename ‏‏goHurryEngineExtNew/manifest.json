{
  "manifest_version": 2,
  "name": "AviGohurryEngine",
  "version": "0.1",
  "description": "גוהרי חשבונאות",
  "icons": {
    "16": "tab-icon.png",
    "32": "tab-icon.png",
    "64": "tab-icon.png",
    "128": "tab-icon.png"
    
  },
   
  "permissions": [
		"activeTab",
		"storage",
		"webNavigation", 
		"webRequest",
    "tabs",
    "downloads"
       
	
	],
  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "Have a good day"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.web-cpa.net/*"
      

      
      ],
      "js": ["jquery.min.js","content.js","FileSaver.js"]
    }
  ]
}